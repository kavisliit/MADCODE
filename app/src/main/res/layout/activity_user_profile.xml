<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".User.user_profile">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/circleImageView_usre"
        android:layout_width="198dp"
        android:layout_height="159dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/greeting">

    </de.hdodenhof.circleimageview.CircleImageView>

    <TextView
        android:id="@+id/u_name"
        android:layout_width="208dp"
        android:layout_height="44dp"
        android:layout_marginTop="72dp"
        android:fontFamily="@font/roboto_medium"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/circleImageView_usre" />

    <TextView
        android:id="@+id/u_name2"
        android:layout_width="208dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto_medium"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/u_name"
        app:layout_constraintTop_toBottomOf="@+id/u_name" />

    <TextView
        android:id="@+id/greeting"
        android:layout_width="266dp"
        android:layout_height="51dp"
        android:onClick="sample"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.043" />

    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="143dp"
        android:layout_marginLeft="143dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="155dp"
        android:layout_marginRight="155dp"
        android:text="Logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/u_phone" />

    <TextView
        android:id="@+id/u_phone"
        android:layout_width="208dp"
        android:layout_height="44dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/roboto_medium"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/u_email"
        app:layout_constraintTop_toBottomOf="@+id/u_email" />

    <Button
        android:id="@+id/sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginLeft="148dp"
        android:layout_marginTop="8dp"
        android:onClick="newone"
        android:text="Button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logout" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:fontFamily="@font/roboto_medium"
        android:text="@string/user_name_app1"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/u_name" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="72dp"
        android:layout_height="32dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/roboto_medium"
        android:text="@string/u_mail"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="72dp"
        android:layout_height="32dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/roboto_medium"
        android:text="@string/user_dob_app"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/u_email"
        android:layout_width="235dp"
        android:layout_height="41dp"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/roboto_medium"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/u_name2"
        app:layout_constraintTop_toBottomOf="@+id/u_name" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="74dp"
        android:layout_height="33dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/roboto_medium"
        android:text="@string/user_num"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

</androidx.constraintlayout.widget.ConstraintLayout>