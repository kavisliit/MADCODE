<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"><![CDATA[
    tools:context=".SharebookViewpage">


    ]]>

    <ImageButton
        android:id="@+id/deleteshare"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="60dp"
        android:backgroundTintMode="multiply"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sbookimg"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/trash"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/editshare"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="52dp"
        android:backgroundTintMode="multiply"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sbookimg"
        app:layout_constraintTop_toBottomOf="@+id/deleteshare"
        app:srcCompat="@drawable/editic"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/sbookimg"
        android:layout_width="392dp"
        android:layout_height="147dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/sbooktext3"
        android:layout_width="265dp"
        android:layout_height="30dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/roboto_medium"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.294"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sbooktext2"
        android:background="@drawable/forget"/>

    <TextView
        android:id="@+id/sbooktext2"
        android:layout_width="265dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto_medium"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.294"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sbookimg"
        android:background="@drawable/forget"/>

    <TextView
        android:id="@+id/sbooktext1"
        android:layout_width="265dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/roboto_medium"
        android:foregroundGravity="center"
        android:gravity="center"
        android:shadowColor="#6E6E6E"
        android:text="TextView"
        android:textColor="@color/fui_bgGitHub"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/sbookimg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />


    <ScrollView
        android:id="@+id/scrollViewS"
        android:layout_width="325dp"
        android:layout_height="456dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sbooktext3"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck"
        android:background="@drawable/forget">


        <TextView
            android:id="@+id/sbooktext4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:text="textMultiLine"
            android:background="@drawable/forget"/>
    </ScrollView>

    <ImageButton
        android:id="@+id/btnBack"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="5dp"
        android:backgroundTintMode="screen"
        android:contentDescription="TODO"
        android:onClick="SharebooktoSharemenu"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/sbookimg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/backbttn" />
</androidx.constraintlayout.widget.ConstraintLayout>